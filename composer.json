{
    "name": "masterix21/laravel-html2pdf",
    "version": "1.0.2",
    "description": "Laravel package for html2pdf.fr",
    "keywords": ["html2pdf", "pdf", "laravel"],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "spipu/html2pdf": ">=5.2.0",
        "laravel/framework": ">=5.4"
    },
    "authors": [
        {
            "name": "Luca Longo",
            "email": "l.longo@ambita.it"
        }
    ],
    "autoload": {
        "psr-4": {
            "masterix21\\html2pdf\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "masterix21\\html2pdf\\ServiceProvider"
            ],
            "aliases": {
                "PDF": "masterix21\\html2pdf\\Facade"
            }
        }
    }
}
